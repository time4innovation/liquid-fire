{{#each this.versions as |version|~}}
  {{#if (lf-or this.renderWhenFalse version.value)~}}
    {{#liquid-child
      version=version
      liquidChildDidRender=(action "childDidRender")
      class=this.class
      id=this.liquidChildId
    ~}}
      {{yield version.value~}}
    {{/liquid-child}}
  {{/if}}
{{/each}}